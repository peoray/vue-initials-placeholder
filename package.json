{
  "name": "vue-initials-placeholder",
  "version": "1.0.0",
  "description": "A Vue component that uses parsed initials as placeholders. Can be used as a substitute for absent user profile photo",
  "private": false,
  "author": "Emmanuel Raymond",
  "homepage": "https://github.com/peoray/vue-initials-placeholder#readme",
  "main": "dist/vue-initials-placeholder.ssr.js",
  "browser": "dist/vue-initials-placeholder.esm.js",
  "module": "dist/vue-initials-placeholder.esm.js",
  "unpkg": "dist/vue-initials-placeholder.min.js",
  "license": "MIT",
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "initials-placeholder"
  ],
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/peoray/vue-initials-placeholder.git"
  },
  "bugs": {
    "url": "https://github.com/peoray/vue-initials-placeholder/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "prepublishOnly": "$npm_execpath build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
